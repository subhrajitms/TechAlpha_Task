package org.jsp.techalpha;

public class RemoveLeadingZeros {
   public static void main(String[] args) {
	   String s="000001203789";
	    String ansString="";
	    for(int i=0;i<s.length();i++)
	    {
	    	if(s.charAt(i)=='0')
	    	{
	    		continue;
	    	}
	    	else
	    	{
	    		ansString+=s.charAt(i);
	    	}
	    	
	    }
	    System.out.println(ansString);
}
}
